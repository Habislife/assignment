#include <stdio.h>
   
int trainglenumber=0;
int number;
int main()

{
	printf("the number of n th term in number/n");
	printf("number/n");
	scanf("%d", &number);
int i ;
for(i=1; i<=number;i=i+1)
{
trainglenumber=trainglenumber+i;
	
}
printf("the nth trainglenumber is %d \n",trainglenumber);
    return 0;
}
